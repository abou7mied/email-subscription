{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [],
  "rules": {
    "quotes": [
      1,
      "single"
    ],
    "class-methods-use-this": 0
  }
}
